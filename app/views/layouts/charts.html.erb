<?xml version="1.0"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<meta http-equiv="content-language" content="en" />
	<meta name="robots" content="noindex,nofollow" />
	<link rel="stylesheet" media="screen,projection" type="text/css" href="/adminiziolite/css/reset.css" /> <!-- RESET -->
	<link rel="stylesheet" media="screen,projection" type="text/css" href="/adminiziolite/css/main.css" /> <!-- MAIN STYLE SHEET -->
	<link rel="stylesheet" media="screen,projection" type="text/css" href="/adminiziolite/css/2col.css" title="2col" /> <!-- DEFAULT: 2 COLUMNS -->
	<link rel="alternate stylesheet" media="screen,projection" type="text/css" href="/adminiziolite/css/1col.css" title="1col" /> <!-- ALTERNATE: 1 COLUMN -->
	<!--[if lte IE 6]><link rel="stylesheet" media="screen,projection" type="text/css" href="/adminiziolite/css/main-ie6.css" /><![endif]--> <!-- MSIE6 -->
	<link rel="stylesheet" media="screen,projection" type="text/css" href="/adminiziolite/css/style.css" /> <!-- GRAPHIC THEME -->
	<link rel="stylesheet" media="screen,projection" type="text/css" href="/adminiziolite/css/mystyle.css" /> <!-- GRAPHIC THEME -->
	<script type="text/javascript" src="/adminiziolite/js/jquery.js"></script>
	<script type="text/javascript" src="/adminiziolite/js/switcher.js"></script>
	<script type="text/javascript" src="/adminiziolite/js/toggle.js"></script>
	<script type="text/javascript" src="/adminiziolite/js/ui.core.js"></script>
	<script type="text/javascript" src="/adminiziolite/js/ui.tabs.js"></script>
	<% if false %>
	<script type="text/javascript">
	$(document).ready(function(){
		$(".tabs > ul").tabs();
	});
	</script>
	<% end %>
	<script type="text/javascript" src="/javascripts/prototype.js"></script>
	<script type="text/javascript" src="/javascripts/application.js"></script>
	<title><%= truncate(@page_title, :length => 65) || 'TradeCalc - Share trading calculator' %></title>
	<% if RAILS_ENV=='production' %>
  <script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-23514654-1']);
    _gaq.push(['_trackPageview']);

    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

  </script>
  <% end %>
</head>

<body>

<div id="main">


	<hr class="noscreen" />

	<!-- Columns -->
	<div id="cols" class="box" style="margin:15px; background:none;">

		<!-- Aside (Left Column) -->

		<hr class="noscreen" />

		<!-- Content (Right Column) -->
		<div id="content" class="box">

      <% if flash[:notice] %><p class="msg done"><%= flash[:notice] %></p><% end %>
      <%= @content_for_layout %>

		</div> <!-- /content -->

	</div> <!-- /cols -->

	<hr class="noscreen" />

	<!-- Footer -->
	<div id="footer" class="box">

		<p class="f-left">&copy; 2011 <a href="#">Trade Calculations</a>, All Rights Reserved &reg;</p>

		<p class="f-right">Templates by <a href="http://www.adminizio.com/">Adminizio</a></p>

	</div> <!-- /footer -->

</div> <!-- /main -->

</body>
</html>
